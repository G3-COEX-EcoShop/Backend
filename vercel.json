{
    "version": 2,
   
    "buildCommand": "npm run installer",
    "installCommand": "npm run installer",
    "builds": [
        {
            "src": "index.js",
            "use": "npm run installer && npm run start"

        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest":"index.js"
        }
    ]
}